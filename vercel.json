{
  "functions": {
    "api/**/*.[jt]s": {
      "maxDuration": 30,
      "includeFiles": "node_modules/@scalar/fastify-api-reference/dist/**"
    },
    "src/**/*.[jt]s": {
      "maxDuration": 30,
      "includeFiles": "node_modules/@scalar/fastify-api-reference/dist/**"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index.js"
    }
  ]
}